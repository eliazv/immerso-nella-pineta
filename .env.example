# Configurazione Alloggiati Web
# Copia questo file in .env.local e compila con i tuoi valori

# EmailJS Configuration
NEXT_PUBLIC_EMAILJS_SERVICE_ID=your_service_id_here
NEXT_PUBLIC_EMAILJS_TEMPLATE_ID=your_template_id_here
NEXT_PUBLIC_EMAILJS_PUBLIC_KEY=your_public_key_here

# Google Apps Script Configuration
NEXT_PUBLIC_GOOGLE_SCRIPT_URL=https://script.google.com/macros/s/your_script_id/exec

# Webhook Configuration (alternativa)
NEXT_PUBLIC_WEBHOOK_URL=https://hooks.zapier.com/hooks/catch/your_webhook_id/

# Email destinatario per le notifiche
NEXT_PUBLIC_NOTIFICATION_EMAIL=zavattaelia@gmail.com

# Google Drive Folder ID (opzionale)
NEXT_PUBLIC_DRIVE_FOLDER_ID=your_folder_id_here

# GitHub Token per Gist (opzionale - per backup file)
NEXT_PUBLIC_GITHUB_TOKEN=your_github_token_here
